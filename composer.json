{
    "name": "omh/laragen",
    "description": "Laragen is an open source package that gives the ability of generating a couple file structure for your Laravel application",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammed Hicham",
            "email": "mohammed@medhicham.com"
        }
    ],
    "require": {
        "symfony/yaml": "^5.0",
        "illuminate/filesystem": "^6.8",
        "illuminate/support": "^6.8",
        "illuminate/console": "^6.8"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Laragen\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laragen\\Tests\\" : "test/"
        }
    }
}
